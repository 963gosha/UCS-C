{
    "extends": ["tslint:recommended"],
    "rules": {
        "arrow-parens": [true, "ban-single-arg-parens"],
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "import-spacing": false,
        "member-access": false,
        "no-empty": false,
        "semicolon": [true, "never"],
        "quotemark": [true, "single", "avoid-escape"]
    }
}
